SendPacket(2, "action|input\n|text|`2Successfully loaded `0[`b[ DOM ]`0] `9Proxy")
function bersih(str)
    local cleanedStr = string.gsub(str, "`(%S)", '')
    cleanedStr = string.gsub(cleanedStr, "`{2}|(~{2})", '')
    return cleanedStr
end
function per(id)
    for _, inv in pairs(GetInventory()) do
    if inv.id == id then
    return inv.amount
    end
    end
    return 0
    end
WL = per(242)
DL = per(1796)
BGL = per(7188)
BLCK = per(11550)
function log(str)
    LogToConsole("[`b[ DOM ]`0]"..str)
    end
