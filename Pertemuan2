        end
    end
    AddHook("OnSendPacket", "wb", wb)
    function woff(b, b)
        if b == ("action|input\n|text|/woff") then
            mode_wm = "off"
            ConsoleLog("`wWrench mode `4Deactive")
            ConsoleLog("`4Deactive `wWrench mode")
            ovl("`4Deactive `wWrench mode")
            RemoveHook("Wrench")
            return true
        end
    end
    AddHook("OnSendPacket", "woff", woff)
    function Wrench_mode(b, b)
        if b:find("buttonClicked|wrench_pull") then
            ConsoleLog("`wWrench mode = `2Pull")
            mode_wm = "pull"
            function Wrench(b)
                if b[0] == "OnDialogRequest" and b[1]:find("add_button|pull|`5Pull``|0|0|") then
                    x = b[1]
                    x2 = x:match("embed_data|netID|(%d+)")
                    SendPacket(2, "action|dialog_return\ndialog_name|popup\nnetID|" .. x2 .. "|\nbuttonClicked|pull")
                    return true
                end
            end
            AddHook("OnVariant", "Wrench", Wrench)
            return true
        elseif b:find("buttonClicked|wrench_kick") then
            ConsoleLog("`wWrench mode = `4Kick")
            mode_wm = "kick"
            function Wrench(b)
                if b[0] == "OnDialogRequest" and b[1]:find("add_button|pull|`5Pull``|0|0|") then
                    x = b[1]
                    x2 = x:match("embed_data|netID|(%d+)")
                    SendPacket(2, "action|dialog_return\ndialog_name|popup\nnetID|" .. x2 .. "|\nbuttonClicked|kick")
                    return true
                end
            end
            AddHook("OnVariant", "Wrench", Wrench)
            return true
        elseif b:find("buttonClicked|wrench_ban") then
            ConsoleLog("`wWrench mode = `4Ban")
            mode_wm = "ban"
            function Wrench(b)
                if b[0] == "OnDialogRequest" and b[1]:find("add_button|pull|`5Pull``|0|0|") then
                    x = b[1]
                    x2 = x:match("embed_data|netID|(%d+)")
                    SendPacket(
                        2,
                        "action|dialog_return\ndialog_name|popup\nnetID|" .. x2 .. "|\nbuttonClicked|world_ban"
                    )
                    return true
                end
            end
            AddHook("OnVariant", "Wrench", Wrench)
            return true
        elseif b:find("buttonClicked|wrench_off") then
            mode_wm = "off"
            ConsoleLog("`wWrench mode `4Deactive")
            RemoveHook("Wrench")
            return true
        end
    end
    AddHook("OnSendPacket", "Wrench_mode", Wrench_mode)

    function fc(b, b)
        if b == ("action|input\n|text|/fc") then
            ConsoleLog("`8Closing proxy.")
            RemoveHooks()
            return true
        end
    end
    AddHook("OnSendPacket", "fc", fc)
    function string.removeColors(b)
        return b:gsub("`.", "")
    end
    
    cbgl_mode = false
    function change_bgl(b)
        if cbgl_mode then
            if b[0] == "OnDialogRequest" and b[1]:find("Dial a number to call somebody in Growtopia") then
                SendPacket(
                    2,
                    "action|dialog_return\ndialog_name|telephone\nnum|53785|\nx|" ..
                        b[1]:match("embed_data|x|(%d+)") ..
                            "|\ny|" .. b[1]:match("embed_data|y|(%d+)") .. "|\nbuttonClicked|bglconvert"
                )
                log("`2Successfully `wConvert 100 `1Diamond Locks `wfor 1 `eBGL`w!")
                return true
            end
        end
    end
    AddHook("OnVariant", "change_bgl", change_bgl)
    function cbgl_hook_xd(b, b)
        if b == ("action|input\n|text|/cdl") then
            if cbgl_mode == true then
                cbgl_mode = false
                ConsoleLog("[`b[ DOM ]`0] `4Converter `0Deactivated!")
                SendPacket(2,"action|input\n|text|[`b[ DOM ]`0] `9Converter `4Deactivated!")
                RemoveHook("change_bgl")
            elseif cbgl_mode == false then
                cbgl_mode = true
                ConsoleLog("[`b[ DOM ]`0] `2Converter `0Activated!")
                SendPacket(2,"action|input\n|text|[`b[ DOM ]`0] `9Converter `2Activated!")
                AddHook("OnVariant", "change_bgl", change_bgl)
            end
            return true
        end
    end
    AddHook("OnSendPacket", "cbgl_hook_xd", cbgl_hook_xd)
    function chnage_bgl2(b)
        if b[0] == "OnDialogRequest" and b[1]:find("add_label_with_icon|big|`wBlue Gem Lock````|left|7188|") then
            return true
        end
    end
    AddHook("OnVariant", "chnage_bgl2", chnage_bgl2)

