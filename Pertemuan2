--update auto pull when enter
local autoPullEnabled = false

AddHook("OnSendPacket", "TOGGLE_AUTOPULL", function(_, str)
    if str:find("/ap1") then
        autoPullEnabled = true
        log("`2Auto Pull Activated")
        ovl("`2Auto Pull Activated")
        return true
    elseif str:find("/ap2") then
        autoPullEnabled = false
        log("`4Auto Pull Deactivated")
        ovl("`4Auto Pull Deactivated")
        return true
    end
end)

AddHook("OnVariant", "AUTOPULL_PLAYER_ENTER", function(var)
    if not autoPullEnabled then return end

    if var[0] == "OnSpawn" and var[1]:find("netID|") then
        local netid = tonumber(var[1]:match("netID|(%d+)"))
        if netid and netid ~= GetLocal().netid then
            SendPacket(2, "action|dialog_return\ndialog_name|popup\nnetID|" .. netid .. "|\nbuttonClicked|pull")
            log("`9Pulled player with NetID: " .. netid)
            ovl("`9Pulled player with NetID: " .. netid)
        end
    end
end)
--end auto pull when enter
AddHook("OnSendPacket", "wm", wm)
function wp(b, b)
    if b == ("action|input\n|text|/wpull") then
        mode_wm = "pull"
        ConsoleLog("`wWrench mode = `2Pull")
        ConsoleLog("`wPull mode `2Activated")
        ovl("`wPull mode `2Activated")
        function Wrench(b)
            if b[0] == "OnDialogRequest" and b[1]:find("add_button|pull|`5Pull``|0|0|") then
                x = b[1]
                x2 = x:match("embed_data|netID|(%d+)")
                SendPacket(2, "action|dialog_return\ndialog_name|popup\nnetID|" .. x2 .. "|\nbuttonClicked|pull")
                ovl("`cPulled Player netID "..x2.."")
                SendPacket(2, "action|input\n|text|[`b[ DOM ]`0] `5Pulled Player")

                return true
            end
        end
        AddHook("OnVariant", "Wrench", Wrench)
        return true
    end
end
AddHook("OnSendPacket", "wp", wp)
    function wk(b, b)
        if b == ("action|input\n|text|/wk") then
            mode_wm = "kick"
            ConsoleLog("`wWrench mode = `4Kick")
            ConsoleLog("`4Kick `wmode `2Activated")
            function Wrench(b)
                if b[0] == "OnDialogRequest" and b[1]:find("add_button|pull|`5Pull``|0|0|") then
                    x = b[1]
