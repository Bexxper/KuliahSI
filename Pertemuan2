    function depo_success_checker(var)
        if var[0] == "OnTalkBubble" and var[1] == GetLocal().netid then
            if var[2]:find("Deposited") and var[2]:find("Blue Gem Locks!") then
                local amount = var[2]:match("Deposited `%$(%d+)`` Blue Gem Locks!")
                amount = amount or "?"
                local message = "[`b[ DOM ]`0] `2Successfully deposited `e" .. amount .. " `9BGL!"
                SendPacket(2, "action|input\n|text|" .. message)
                ConsoleLog(message)
                ovl(message)
                return true
            end
        end
    end
    AddHook("OnVariant", "depo_success_checker", depo_success_checker)
    
