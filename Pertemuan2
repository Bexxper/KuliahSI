function ovl(str)
SendVariantList({ [0] = "OnTextOverlay", [1] = str })
end
    
AddHook("onvariant", "OTHERVAR", function(var)
    if var[0]:find("OnConsoleMessage") and var[1]:find("World Locked") then
    SendVariantList({
    [0] = "OnAddNotification",
    [1] = "interface/atomic_button.rttex",
    [2] = "[`c[ DOM ]`0]\n`1Visit Aawastore",
    [3] = "audio/hub_open.wav"
    })
    SendPacket(2, "action|input\n|text|[`b[ DOM ]`0] Proxy `2Successfully loaded `w[`b[ DOM ]`0] WORLD : `#CPS `w[ `oVisit Aawastore `w]") ---11
    end
end)
    ---function drop
    function drop(id,amount)
        SendPacket(2, "action|dialog_return\ndialog_name|drop\nitem_drop|"..id.."|\nitem_count|"..amount)
        end

    AddHook("onsendpacket", "INFORMATION", function(type,str)
        if str:find("/proxy") then
        ovl("Opening Proxy Menu")
        ProxyCommand()
        return true
        end
    end)

    function findItem(id)
        for _, itm in pairs(GetInventory()) do
        if itm.id == id then
        return itm.amount
        end
        end
        return 0
    end
    ---drop section
    ---drop dl
    ---drop wl
    ---drop blue gem lock
    ---drop black
    function DropblackCommand(amount)
        local blackID = 11550 
    
        if amount and amount > 0 then
           
            SendPacket(2, "action|dialog_return\ndialog_name|drop\nitem_drop|" .. blackID .. "|\nitem_count|" .. amount)
            Sleep(100) 
            SendPacket(2, "action|input\n|text|[`b[ DOM ]`0] `2Successfully `0Drop " .. amount .. " `bBLACK GEM LOCK")
            log("`2Successfully `0Drop " .. amount .. " `bBLACK GEM LOCK")
        else
            SendPacket(2, "action|input\n|text|[`b[ DOM ]`0] `4Invalid amount. Use `/dbl <amount>`")
        end
    end
    
    AddHook("onsendpacket", "DROP_BLACK_COMMAND", function(type, str)
        if str:find("/dbl") then
            local amount = tonumber(str:match("/dbl%s*(%d+)")) 
            DropblackCommand(amount)
            return true
        end
    end)
    
    ---spin wheel
-- Inisialisasi tabel jika belum ada
local HostApa, ActivityLog, LogSpin = "", {}, {}, false, false, false

function FaceSide(b)
local z = 32 if b == "left" then z = 48 elseif b == "right" then z = 32 end

local pkt = {}
pkt.type = 0
pkt.x = GetLocal().pos.x
pkt.y = GetLocal().pos.y
pkt.state = z
SendPacketRaw(false,pkt)
Sleep(50)
end

function GetName(id) for _, player in pairs(GetPlayerList()) do if player.netid == id then return player.name end end end 
function ActivityLogPage() Action = {} for _, log in pairs(ActivityLog) do table.insert(Action, log.act) end SendVariantList({[0] = "OnDialogRequest", [1] = "set_default_color|`w|\nadd_label_with_icon|big|Logs Drop|left|10794|\nadd_spacer|small|\n"..table.concat(Action).."\nadd_spacer|small|\nadd_quick_exit|||\nend_dialog|ActivityLog|Close||"}) end
function FilterSpin(id) FilterLog = {} for _, log in pairs(LogSpin) do if log.netid == id then table.insert(FilterLog,"\nadd_label_with_icon|small|"..log.spin.."|left|758|\n") end end SendVariantList({[0] = "OnDialogRequest", [1] = "set_default_color|`w|\nadd_label_with_icon|big|"..GetName(id).." Spin Logs|left|758|\nadd_spacer|small|\n"..table.concat(FilterLog).."\nadd_spacer|small|\nadd_button|/cctv|Exit|noflags|0|0|\nadd_quick_exit|||\nend_dialog|FilterSpin|||"}) end
function SpinLog() SpunLog = {} for _, spin in pairs(LogSpin) do table.insert(SpunLog, spin.spin) end SendVariantList({[0] = "OnDialogRequest", [1] = "set_default_color|`w|\nadd_label_with_icon|big|Spin Logs|left|758|\nadd_spacer|small|\nadd_smalltext|Press wheel button to focus player spin Logs|\n"..table.concat(SpunLog).."\nadd_spacer|small|\nadd_quick_exit|||\nend_dialog|WorldSpin|Close||"}) end---spinlog update

AddHook("OnSendPacket","Hook", function(b, b)
if b:find("/reme") then if HostApa ~= "reme" then HostApa = "reme" log("`4Wheel Spin `9Mode set to : `9REME") ovl("`4Wheel Spin `9Mode set to : `9REME") else HostApa = "" log("`4Wheel Spin `9Mode set to : `0Normal") ovl("`4Wheel Spin `9Mode set to : `0Normal")
end 
return true
end
if b:find("/leme") then if HostApa ~= "leme" then HostApa = "leme" log("`4Wheel Spin `9Mode set to : `9LEME") ovl("`4Wheel Spin `9Mode set to : `9LEME") else HostApa = "" log("`4Wheel Spin `9Mode set to : `0Normal") ovl("`4Wheel Spin `9Mode set to : `0Normal")
end 
return true
end 
if b:find("/qeme") then if HostApa ~= "qeme" then HostApa = "qeme" log("`4Wheel Spin `9Mode set to: `1QEME") ovl("`4Wheel Spin `9Mode set to : `9QEME") else HostApa = "" log("`4Wheel Spin `9Mode set to : `0Normal") ovl("`4Wheel Spin `9Mode set to : `0Normal")
end 
return true
end 
if b:find("/ceme") then if HostApa ~= "ceme" then HostApa = "ceme" log("`4Wheel Spin `9Mode set to: `1CEME") ovl("`4Wheel Spin `9Mode set to : `9CEME") else HostApa = "" log("`4Wheel Spin `9Mode set to : `0Normal") ovl("`4Wheel Spin `9Mode set to : `0Normal")
end 
return true
end 
if b:find("/logspin") then SpinLog() return true end
if b:find("/logsdrop") then ActivityLogPage() return true end
if b:find("dialog_name|WorldSpin\nbuttonClicked|(%d+)") then NetID = tonumber(b:match("dialog_name|WorldSpin\nbuttonClicked|(%d+)")) FilterSpin(NetID) return true end
end)

AddHook("OnVariant","Hook",function(b)
if b[0] == "OnConsoleMessage" and b[1]:find("`6<(.+)") then return false
elseif b[0] == "OnConsoleMessage" and b[1]:find("Collected  `w(%d+) World Lock") then AmountCollectWL = tonumber(b[1]:match("Collected  `w(%d+) World Lock")) ovl("`2Collected `w"..AmountCollectWL.." `9World Locks") SendPacket(2, "action|input\n|text|[`b[ DOM ]`w] `5Collect `w"..AmountCollectWL.." `9WL")
    table.insert(ActivityLog, {act = "\nadd_label_with_icon|small|"..GetLocal().name.." `oCollected `w"..AmountCollectWL.." `9World Lock `oat "..os.date("%H:%M on %d/%m").."|left|10794|\n", netid = GetLocal().netID, acts = "Collected `w" ..AmountCollectWL.. " `9World Lock"})
    return true
    elseif b[0] == "OnConsoleMessage" and b[1]:find("Collected  `w(%d+) Diamond Lock") then AmountCollectDL = tonumber(b[1]:match("Collected  `w(%d+) Diamond Lock")) ovl("`2Collected `w"..AmountCollectDL.." `1DL") SendPacket(2, "action|input\n|text|[`b[ DOM ]`w] `5Collect `w"..AmountCollectDL.." `1DL")
    table.insert(ActivityLog, {act = "\nadd_label_with_icon|small|"..GetLocal().name.." `oCollected `w"..AmountCollectDL.." `1Diamond Lock `oat "..os.date("%H:%M on %d/%m").."|left|10794|\n", netid = GetLocal().netID, acts = "Collected `w" ..AmountCollectDL.. " `1Diamond Lock"})
    return true
    elseif b[0] == "OnConsoleMessage" and b[1]:find("Collected  `w(%d+) Blue Gem Lock") then AmountCollectBGL = tonumber(b[1]:match("Collected  `w(%d+) Blue Gem Lock")) ovl("`2Collected `w"..AmountCollectBGL.." `eBGL") SendPacket(2, "action|input\n|text|[`b[ DOM ]`w] `5Collect `w"..AmountCollectBGL.." `eBGL")
    table.insert(ActivityLog, {act = "\nadd_label_with_icon|small|"..GetLocal().name.." `oCollected `w"..AmountCollectBGL.." `eBlue Gem Lock `oat "..os.date("%H:%M on %d/%m").."|left|10794|\n", netid = GetLocal().netID, acts = "Collected `w" ..AmountCollectBGL.. " `eBlue Gem Lock"})
    return true
    elseif b[0] == "OnConsoleMessage" and b[1]:find("Collected  `w(%d+) Black Gem Lock") then AmountCollectBBGL = tonumber(b[1]:match("Collected  `w(%d+) Black Gem Lock")) ovl("`2Collected `w"..AmountCollectBBGL.." `bBLACK") SendPacket(2, "action|input\n|text|[`b[ DOM ]`w] `5Collect `w"..AmountCollectBBGL.." `bBLACK")
    table.insert(ActivityLog, {act = "\nadd_label_with_icon|small|"..GetLocal().name.." `oCollected `w"..AmountCollectBBGL.." `bBlack Gem Lock `oat "..os.date("%H:%M on %d/%m").."|left|10794|\n", netid = GetLocal().netID, acts = "Collected `w" ..AmountCollectBBGL.. " `bBlack Gem Lock"})
    return true
elseif b[0] == "OnSDBroadcast" then log("`1Super Duper Broadcast `4Blocked") ovl("`1Super Duper Broadcast `4Blocked")
return true
elseif b[0] == "OnTalkBubble" and b[1] == GetLocal().netid and b[2]:find("`2Withdrawn") and b[2]:find("You have (%d+) in the bank now.") then AmountWithdrawn = b[2]:match("`2Withdrawn (%d+) Blue Gem Locks!") log("[`b[DOM]`0] `9Withdrawn `w"..AmountWithdrawn.." `eBlue Gem Lock") return true
elseif b[0] == "OnTalkBubble" and b[3] ~= -1 and b[2]:find("spun the wheel and got") then
    if b[2]:find("``6>``") then
        p = {}
        p[0] = "OnTalkBubble"
        p[1] = b[1]
        p[2] = "`0[`b[ DOM ]`0]`0[`4FAKE`0] " .. b[2]
        p[3] = -1
        SendVariantList(p) table.insert(LogSpin, {spin = "\nadd_label_with_icon_button|small|`0[`b[ DOM ]`0]`0[`4FAKE`0] " .. b[2].."|left|758|"..b[1].."|\n", netid = b[1], spins = "`0[`b[ DOM ]`0]`0[`4FAKE`0] " .. b[2]})
        elseif b[2]:find("``!]``") then
        s2 = b[2]:find("`` spun") - 1
        s1 = b[2]:sub(4, s2)
        jml1 = b[2]:find("got ") + 6
        jml2 = b[2]:find("``!") - 1
        jml = b[2]:sub(jml1, jml2)
        if HostApa == "qeme" then
            local raw_jml = tonumber(jml) or 0
            local qeme_digit = raw_jml % 10
            local x3_label = ""
        
            -- KASUS KHUSUS: Jika jml == 0 → QEME : 1 (bukan 0)
            if raw_jml == 0 then
                qeme_digit = 1
            -- Jika digit = 0 dan jml bukan 0 (misal: 10, 20, 30) → QEME : 0 + [X3]
            elseif qeme_digit == 0 then
                x3_label = "`c[X3]"
            end
        
            -- Format warna
            local qeme = ""
            if qeme_digit <= 3 then 
                qeme = "`4" .. qeme_digit
            elseif qeme_digit <= 6 then 
                qeme = "`9" .. qeme_digit
            elseif qeme_digit <= 9 then 
                qeme = "`2" .. qeme_digit
            end
        
            -- Tampilkan Talk Bubble
            p = {}
            p[0] = "OnTalkBubble"
            p[1] = b[1]
            p[2] = "`0[`b[ DOM ]`0]`0[`2REAL`0] (`5" .. jml .. "`w) `w[`2QEME : "..qeme.."`w] " .. x3_label
            p[3] = -1
            SendVariantList(p)
        
            -- Log Spin
            table.insert(LogSpin, {
                spin = "\nadd_label_with_icon_button|small|`0[`b[ DOM ]`0]`0[`2REAL`0] " .. b[2] .. " `w[`2QEME : "..qeme.."`w] "..x3_label.."|left|758|"..b[1].."|\n",
                netid = b[1],
                spins = "`0[`b[ DOM ]`0]`0[`2REAL`0] " .. b[2] .. " `w[`2QEME : "..qeme.."`w] "..x3_label
            })
        elseif HostApa == "reme" then
            Num1 = math.floor(jml / 10)
            Num2 = jml % 10
            reme = Num1 + Num2
            if reme > 10 then
                reme = reme % 10
            elseif reme == 10 then
                reme = "`20"
            elseif reme == 0 then
                reme = "`20"
            end
            if tonumber(reme) and reme >= 7 and reme <= 9 then
                reme = "`2" .. reme
            elseif tonumber(reme) and reme >= 4 and reme <= 6 then
                reme = "`9" .. reme
            elseif tonumber(reme) and reme >= 2 and reme <= 3 then
                reme = "`4" .. reme
            elseif tonumber(reme) and reme == 1 then
                reme = "`4" .. reme
            end
            local x3_label = ""
            if tonumber(jml) == 19 or tonumber(jml) == 28 or tonumber(jml) == 0 then
                x3_label = " `c[X3]"
            end
        p = {}
        p[0] = "OnTalkBubble"
        p[1] = b[1]
        p[2] = "`0[`b[ DOM ]`0]`0[`2REAL`0] (`5" .. jml .."`w) `w[`9REME `w: "..reme.."`w]" .. x3_label
        p[3] = -1
        SendVariantList(p) table.insert(LogSpin, {spin = "\nadd_label_with_icon_button|small|`0[`b[ DOM ]`0]`0[`2REAL`0] " .. b[2].." `w[`9REME `w: "..reme.."`w]"..x3_label.."|left|758|"..b[1].."|\n", netid = b[1], spins = "`0[`b[ DOM ]`0]`0[`2REAL`0] " .. b[2].." `w[`9REME `w: "..reme.."`w]"..x3_label})
    elseif HostApa == "leme" then
        Num1 = math.floor(jml / 10)
        Num2 = jml % 10
        leme = Num1 + Num2
        if leme > 10 then
            leme = leme % 10
        elseif leme == 10 then
            leme = "`20"
        elseif leme == 0 then
            leme = "`20"
        end
        if tonumber(leme) and leme >= 7 and leme <= 9 then
            leme = "`2" .. leme
        elseif tonumber(leme) and leme >= 4 and leme <= 6 then
            leme = "`9" .. leme
        elseif tonumber(leme) and leme >= 2 and leme <= 3 then
            leme = "`4" .. leme
        elseif tonumber(leme) and leme == 1 then
            leme = "`4" .. leme
        end
    
        local leme_label = ""
        if tonumber(jml) == 1 or tonumber(jml) == 29 or tonumber(jml) == 10 then
            leme_label = " `c[X3]"
        elseif tonumber(jml) == 19 or tonumber(jml) == 28 or tonumber(jml) == 0 then
            leme_label = " `c[X4]"
        end
    
        p = {}
        p[0] = "OnTalkBubble"
        p[1] = b[1]
        p[2] = "`0[`b[ DOM ]`0]`0[`2REAL`0] (`5" .. jml .."`w) `w[`6LEME : "..leme.."`w]" .. leme_label
        p[3] = -1
        SendVariantList(p)
        table.insert(LogSpin, {
            spin = "\nadd_label_with_icon_button|small|`0[`b[ DOM ]`0]`0[`2REAL`0] " .. b[2].." `w[`6LEME : "..leme.."`w]"..leme_label.."|left|758|"..b[1].."|\n",
            netid = b[1],
            spins = "`0[`b[ DOM ]`0]`0[`2REAL`0] " .. b[2].." `w[`6LEME : "..leme.."`w]"..leme_label
        })
    elseif HostApa == "ceme" then
        local Num1 = math.floor(jml / 10)
        local Num2 = jml % 10
        local ceme = Num1 + Num2
    
        if ceme > 10 then
            ceme = ceme % 10
        elseif ceme == 10 or ceme == 0 then
            ceme = "`20"
        end
    
        if tonumber(ceme) and ceme >= 7 and ceme <= 9 then
            ceme = "`2" .. ceme
        elseif tonumber(ceme) and ceme >= 4 and ceme <= 6 then
            ceme = "`9" .. ceme
        elseif tonumber(ceme) and ceme >= 2 and ceme <= 3 then
            ceme = "`4" .. ceme
        elseif tonumber(ceme) and ceme == 1 then
            ceme = "`4" .. ceme
        end
    
        local ceme_label = ""
        local j = tonumber(jml)
        if j == 0 or j == 19 or j == 28 then
            ceme_label = " `c[X4]"
        elseif j == 1 or j == 10 or j == 29 or j == 2 or j == 11 or j == 20 then
            ceme_label = " `c[X3]"
        end
    
        p = {}
        p[0] = "OnTalkBubble"
        p[1] = b[1]
        p[2] = "`0[`b[ DOM ]`0]`0[`2REAL`0] ("..jml..") `w[`6CEME : "..ceme.."`w]"..ceme_label
        p[3] = -1
        SendVariantList(p)
        table.insert(LogSpin, {
            spin = "\nadd_label_with_icon_button|small|`0[`b[ DOM ]`0]`0[`2REAL`0] "..b[2].." `w[`6CEME : "..ceme.."`w]"..ceme_label.."|left|758|"..b[1].."|\n",
            netid = b[1],
            spins = "`0[`b[ DOM ]`0]`0[`2REAL`0] ("..jml..") `w[`6CEME : "..ceme.."`w]"..ceme_label
        })
    else
        p = {}
        p[0] = "OnTalkBubble"
        p[1] = b[1]
        p[2] = "`0[`b[ DOM ]`0]`0[`2REAL`0] (".. jml .. ") "
        SendVariantList(p) table.insert(LogSpin, {spin = "\nadd_label_with_icon_button|small|`0[`b[ DOM ]`0]`0[`2REAL`0] " .. b[2].."|left|758|"..b[1].."|\n", netid = b[1], spins = "`0[`b[ DOM ]`0]`0[`2REAL`0] " .. b[2]})
        end
        else
        p = {}
        p[0] = "OnTalkBubble"
        p[1] = b[1]
        p[2] = "`0[`b[ DOM ]`0]`0[`4FAKE`0] " .. b[2]
        p[3] = -1
        SendVariantList(p) table.insert(LogSpin, {spin = "\nadd_label_with_icon_button|small|`0[`b[ DOM ]`0]`0[`4FAKE`0] " .. b[2].."|left|758|"..b[1].."|\n", netid = b[1], spins = "`0[`b[ DOM ]`0]`0[`4FAKE`0] " .. b[2]})
        end
return true
end
return false end)

