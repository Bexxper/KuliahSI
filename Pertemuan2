function res(b, b)
    if b == ("action|input\n|text|/res") then
        log("`4Respawning")
        SendPacket(2, "action|respawn")
        SendPacket(2, "action|input\n|text|`0[`b[ DOM ]`0] Respawned. `4BYE `bblyad `5b1tch `bholle")
        return true
    end
end
AddHook("OnSendPacket", "respawn", res)

---champ
chem_mode = false
function champ_bang(b)
    if chem_mode then
        if b[0] == "OnDialogRequest" and b[1]:find("Dial a number to call somebody in Growtopia") then
            SendPacket(
                2,
                "action|dialog_return\ndialog_name|telephone\nnum|53785|\nx|" ..
                    b[1]:match("embed_data|x|(%d+)") ..
                        "|\ny|" .. b[1]:match("embed_data|y|(%d+)") .. "|\nbuttonClicked|getchamp"
            )
            log("`2Successfully `wbuy 1 Champagne!")
            return true
        end
    end
end
AddHook("OnVariant", "champ_bang", champ_bang)
function chmp_hook_xd(b, b)
    if b == ("action|input\n|text|/champ") then
        if chem_mode == true then
            chem_mode = false
            ConsoleLog("[`b[ DOM ]`0] `4Auto Buy Champagne `0Deactivated!")
            SendPacket(2,"action|input\n|text|[`b[ DOM ]`0] `9Auto Buy Champagne `4Deactivated!")
            RemoveHook("champ_bang")
        elseif chem_mode == false then
            chem_mode = true
            ConsoleLog("[`b[ DOM ]`0] `2Auto Buy Champagne `0Activated!")
            SendPacket(2,"action|input\n|text|[`b[ DOM ]`0] `9Auto Buy Champagne `2Activated!")
            AddHook("OnVariant", "champ_bang", champ_bang)
        end
        return true
    end
end
AddHook("OnSendPacket", "chmp_hook_xd", chmp_hook_xd)
function chemp_bang(b)
    if b[0] == "OnDialogRequest" and b[1]:find("One Champagne Bottle cost") then
        return true
    end
end
AddHook("OnVariant", "chemp_bang", chemp_bang)

---droka
function findItem(id)
    for _, itm in pairs(GetInventory()) do
    if itm.id == id then
    return itm.amount
    end
    end
    return 0
    end
    function wrench(x,y)
    pkt = {}
    pkt.type = 3
    pkt.value = 32
    pkt.px = math.floor(GetLocal().pos.x / 32 +x)
    pkt.py = math.floor(GetLocal().pos.y / 32 +y)
    pkt.x = GetLocal().pos.x
    pkt.y = GetLocal().pos.y
    SendPacketRaw(false, pkt)
    end
 
local function b(b, c)
    for b, b in pairs(b) do
        if b == c then
            return true
        end
    end
    return false
end

local function logText(text)
    packet = {}
    packet[0] = "OnConsoleMessage"
    packet[1] = "`8[`9dsc.gg/dracogear`8]`6 ".. text
    SendVariantList(packet)
end

local function FormatNumber(num)
num = math.floor(num + 0.5)
local formatted = tostring(num)
local k = 3
while k < #formatted do
formatted = formatted:sub(1, #formatted - k) .. "," .. formatted:sub(#formatted - k + 1)
k = k + 4
end
return formatted
end

function Logb(str) SendVariantList({[0] = "OnTalkBubble", [1] = GetLocal().netid ,[2] = "`$[ `0"..os.date("%H:%M:%S").."`$] [`b[ DOM ]`0] `w"..str}) end
function ConsoleLog(str) SendVariantList({[0] = "OnConsoleMessage", [1] = "`$[ `0"..os.date("%H:%M:%S").."`$] [`b[ DOM ]`0] `w"..str}) end

function FaceSide(b)
local z = 32 if b == "left" then z = 48 elseif b == "right" then z = 32 end

local pkt = {}
pkt.type = 0
pkt.x = GetLocal().pos.x
pkt.y = GetLocal().pos.y
pkt.state = z
SendPacketRaw(false,pkt)
Sleep(50)
end



function string.removeColors(b) return b:gsub("`.", "") end

function dropes(b, c) SendPacket(2,"action|dialog_return\ndialog_name|drop\nitem_drop|".. b .. "|\nitem_count|".. c) Sleep(180) end

function findItem(id) for _, c in pairs(GetInventory()) do if c.id == id then return c.amount end end return 0 end


function buatireng() SendPacket(2,"action|dialog_return\ndialog_name|info_box\nbuttonClicked|make_bgl") end
function shatterireng() SendPacket(2,"action|dialog_return\ndialog_name|info_box\nbuttonClicked|make_bluegl") end
function shatter(id) local pkt = {} pkt.type = 10 pkt.value = id SendPacketRaw(false, pkt) end 

function cdrop_delayed()
    pecahan = {1000000, 10000, 100, 1}
    black = 0
    bgl = 0
    dl = 0
    wl = 0
    for b = 1, 4 do
    while wls >= pecahan[b] do
    if pecahan[b] == pecahan[1] then
    black = black + 1
    wls = wls - 1000000
    elseif pecahan[b] == pecahan[2] then
    bgl = bgl + 1
    wls = wls - 10000
    elseif pecahan[b] == pecahan[3] then
    dl = dl + 1
    wls = wls - 100
    elseif pecahan[b] == pecahan[4] then
    wl = wl + 1
    wls = wls - 1
    end
    end
    end
    ConsoleLog("[`b[ DOM ]`0] `2Successfully `wDrop " .. black .. " `bBLACK `w" .. bgl .. " `eBGL `w" .. dl .. " `1DLS `wand `w" .. wl .. " `9WL") -- dd
    SendPacket(2, "action|input\n|text|[`b[ DOM ]`0] `2Successfully `wDrop " .. black .. " `bBLACK `w" .. bgl .. " `eBGL `w" .. dl .. " `1DLS `wand `w" .. wl .. " `9WL")
    ovl("[`b[ DOM ]`0] `2Successfully `wDrop " .. black .. " `bBLACK `w" .. bgl .. " `eBGL `w" .. dl .. " `1DLS `wand `w" .. wl .. " `9WL")
    itemamt = {black, bgl, dl, wl}
    itemid = {11550, 7188, 1796, 242}
    --shattering
    if findItem(11550) < black then buatireng() Sleep(100) end
    if findItem(7188) < bgl then shatterireng() Sleep(100) end
    if findItem(1796) < dl then shatter(7188) Sleep(100) end
    if findItem(242) < wl then shatter(1796) Sleep(100) end
    --dropping
    if black > 0 then
        dropes(itemid[1], itemamt[1])
        table.insert(ActivityLog, {act = "\nadd_label_with_icon|small|"..GetLocal().name.." `oDropped `w"..itemamt[1].." `bBlack Gem Lock `oat "..os.date("%H:%M on %d/%m").."|left|10794|\n", netid = GetLocal().netID, acts = "Dropped `w" ..itemamt[1].. " `bBlack Gem Lock"})
    end
    if bgl > 0 then
        dropes(itemid[2], itemamt[2])
        table.insert(ActivityLog, {act = "\nadd_label_with_icon|small|"..GetLocal().name.." `oDropped `w"..itemamt[2].." `eBlue Gem Lock `oat "..os.date("%H:%M on %d/%m").."|left|10794|\n", netid = GetLocal().netID, acts = "Dropped `w" ..itemamt[2].. " `eBlue Gem Lock"})
    end
    if dl > 0 then
        dropes(itemid[3], itemamt[3])
        table.insert(ActivityLog, {act = "\nadd_label_with_icon|small|"..GetLocal().name.." `oDropped `w"..itemamt[3].." `1Diamond Lock `oat "..os.date("%H:%M on %d/%m").."|left|10794|\n", netid = GetLocal().netID, acts = "Dropped `w" ..itemamt[3].. " `1Diamond Lock"})
    end
    if wl > 0 then
        dropes(itemid[4], itemamt[4])
        table.insert(ActivityLog, {act = "\nadd_label_with_icon|small|"..GetLocal().name.." `oDropped `w"..itemamt[4].." `9World Lock `oat "..os.date("%H:%M on %d/%m").."|left|10794|\n", netid = GetLocal().netID, acts = "Dropped `w" ..itemamt[4].. " `9World Lock"})
    end
    --ConsoleLog("`5Dropped `3" .. black .. " `bblacks `3" .. bgl .. " `9bgl `3" .. dl .. " `9dls and `3" .. wl .. " `9wls")
    return true end

function cdrop_drop(b, b)
if b:find("/cd (%d+)") then
wls = (tonumber(b:match("/cd (%d+)")))
customdrop = true
return true
end
if b:find("/db (%d+)") then
    wls = (tonumber(b:match("/db (%d+)"))*10000)
    customdrop = true
    return true
    end
if b:find("/dd (%d+)") then
wls = (tonumber(b:match("/dd (%d+)"))*100)
customdrop = true
return true
end


if b:find("/cbl") then buatireng() 
    SendPacket(2, "action|input\n|text|[`b[ DOM ]`0] `2Successfully converted `bBlack Gem Lock `0to `eBlue Gem Lock!")
return true end
if b:find("/blue") then shatterireng() 
    SendPacket(2, "action|input\n|text|[`b[ DOM ]`0] `2Successfully converted `e100 Blue Gem Locks `0to `bBlack Gem Lock!")
return true end
--update 3.0
end
AddHook("OnSendPacket", "hooking", cdrop_drop)



-- divider

function Data()
Amount = 0
for _, list in pairs(data) do
Name = ""
if list.id == 7188 then
Name = "Blue Gem Lock"
Amount = Amount + list.count * 10000
elseif list.id == 1796 then
Name = "Diamond Lock"
Amount = Amount + list.count * 100
elseif list.id == 242 then
Name = "World Lock"
Amount = Amount + list.count
end
end
data = {}
end




function dropblackuy(b, b)
        if b:find("action|input") then
            text = b:gsub("action|input\n|text|", "")
            if text:find("/") then
                cmd = text:gsub("/", "")
                if cmd:find("dbl") then
                    amount = cmd:gsub("dbl", "")
                    local b = 0
                    b = amount
                    amount = tonumber(amount)
                    for c, c in pairs(GetInventory()) do
                        if c.id == 11550 then
                            if c.amount < amount then
                                ConsoleLog("`8No enough blacks.")
                                ConsoleLog("`8No enough blacks.")
                                return true
                            else
                                ConsoleLog("`wDropped`w" .. b .. " `bBLACK")
                                ConsoleLog("`wDropped`w" .. b .. " `bBLACK")
                                SendPacket(
                                    2,
                                    "action|dialog_return\ndialog_name|drop\nitem_drop|11550|\nitem_count|" .. b
                                )
                                return true
                            end
                        end
                    end
                end
            end
        end
    end
AddHook("OnSendPacket", "dropblackuy", dropblackuy)
